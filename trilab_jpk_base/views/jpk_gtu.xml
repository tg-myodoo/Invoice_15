<?xml version="1.0" encoding="UTF-8"?>
<odoo>
    <record id="jpk_gtu_tree" model="ir.ui.view">
        <field name="name">jpk_gtu_tree</field>
        <field name="model">jpk.gtu</field>
        <field name="arch" type="xml">
            <tree>
                <field name="name"/>
                <field name="description"/>
            </tree>
        </field>
    </record>

    <record id="jpk_gtu_form" model="ir.ui.view">
        <field name="name">jpk_gtu_form</field>
        <field name="model">jpk.gtu</field>
        <field name="arch" type="xml">
            <form>
                <sheet>
                    <h1><field name="name"/></h1>
<!--                    <label for="description"/>-->
                    <field name="description"/>
                </sheet>
            </form>
        </field>
    </record>

</odoo>
